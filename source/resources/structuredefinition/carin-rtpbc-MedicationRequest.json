{
	"resourceType": "StructureDefinition",
	"id": "carin-rtpbc-MedicationRequest",
	"meta": {
		"versionId": "32",
		"lastUpdated": "2019-11-01T06:28:22.000-04:00"
	},
	"text": {
		"status": "additional",
		"div": "<div xmlns=\"http://www.w3.org/1999/xhtml\"><div><div>This profile constrains the MedicationRequest resource to convey a subset of prescription information required in the consumer real-time pharmacy benefit  check (RTPBC) process. The content specifies the prescribed product and quantity, and references the prescribing practitioner and the patients' preferred pharmacy                                  </div></div></div>"
	},
	"extension": [],
	"url": "http://hl7.org/fhir/us/carin/StructureDefinition/rtpbc-request-prescription-info",
	"version": "0.0.1",
	"name": "CarinRtpbcRequestPrescriptionInfo",
	"title": "CARIN RTPBC Request Prescription Information",
	"status": "draft",
	"date": "2019-10-15",
	"publisher": "CARIN Alliance",
	"description": "This profile constrains the MedicationRequest resource to convey a subset of prescription information required in the consumer real-time pharmacy benefit check (RTPBC) process. The content specifies the prescribed product and quantity, and references the prescribing practitioner and the patients' preferred pharmacy",
	"fhirVersion": "4.0.0",
	"kind": "resource",
	"abstract": false,
	"type": "MedicationRequest",
	"baseDefinition": "http://hl7.org/fhir/StructureDefinition/MedicationRequest",
	"derivation": "constraint",
	"differential": {
		"element": [
			{
				"id": "MedicationRequest",
				"path": "MedicationRequest"
			},
			{
				"id": "MedicationRequest.status",
				"path": "MedicationRequest.status",
				"label": "Request Status",
				"short": "Request Status",
				"definition": "Status of the request for dispensing",
				"comment": "Always equals 'draft'",
				"fixedCode": "draft",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.intent",
				"path": "MedicationRequest.intent",
				"label": "Request Intent",
				"short": "Request Intent",
				"definition": "Intent of the request for dispensing",
				"comment": "Always equals 'proposal'",
				"fixedCode": "proposal",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.medication[x]",
				"path": "MedicationRequest.medication[x]",
				"label": "Prescribed Product",
				"short": "Prescribed Product",
				"definition": "A product identifier for the prescribed medication. Either an NDC11 or an RxNorm code for a prescribable product (representing drug name, strength and dose form)",
				"comment": "Code system for RxNorm ishttp://www.nlm.nih.gov/research/umls/rxnorm and the code system for NDC11 is * TBD*",
				"min": 1,
				"max": "1",
				"type": [
					{
						"code": "CodeableConcept"
					}
				],
				"mustSupport": true,
				"binding": {
					"strength": "extensible",
					"description": "CARIN RTPBC prescribable product codes (NDC and RxNorm)",
					"valueSet": "http://hl7.org/fhir/us/carin/ValueSet/carin-rtpbc-prescribable-product-code"
				}
			},
			{
				"id": "MedicationRequest.medicationCodeableConcept.coding",
				"path": "MedicationRequest.medicationCodeableConcept.coding",
				"min": 1,
				"max": "*"
			},
			{
				"id": "MedicationRequest.medicationCodeableConcept.coding.system",
				"path": "MedicationRequest.medicationCodeableConcept.coding.system",
				"min": 1
			},
			{
				"id": "MedicationRequest.medicationCodeableConcept.coding.code",
				"path": "MedicationRequest.medicationCodeableConcept.coding.code",
				"min": 1
			},
			{
				"id": "MedicationRequest.medicationCodeableConcept.coding.display",
				"path": "MedicationRequest.medicationCodeableConcept.coding.display",
				"label": "Product Description",
				"short": "Product Description",
				"definition": "The full prescribable product name, including drug name, strength and dose form. The drug name equals the brand name if the branded product is desired",
				"min": 1
			},
			{
				"id": "MedicationRequest.subject",
				"path": "MedicationRequest.subject",
				"label": "Patient",
				"short": "Patient",
				"definition": "The patient for whom this medication is being requested",
				"type": [
					{
						"code": "Reference",
						"targetProfile": [
							"http://hl7.org/fhir/us/carin/StructureDefinition/rtpbc-request-patient-info"
						]
					}
				],
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.subject.reference",
				"path": "MedicationRequest.subject.reference",
				"label": "Patient Reference",
				"short": "Patient Reference",
				"definition": "Reference to a Patient resource",
				"min": 1,
				"max": "1",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.requester",
				"path": "MedicationRequest.requester",
				"label": "Prescriber",
				"short": "Prescriber",
				"definition": "The prescriber of the medication being requested",
				"min": 1,
				"type": [
					{
						"code": "Reference",
						"targetProfile": [
							"http://hl7.org/fhir/us/carin/StructureDefinition/rtpbc-request-prescriber-info"
						]
					}
				],
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.requester.reference",
				"path": "MedicationRequest.requester.reference",
				"label": "Prescriber Reference",
				"short": "Prescriber Reference",
				"definition": "Reference to a Practitioner resource",
				"min": 1,
				"max": "1",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.reasonCode",
				"path": "MedicationRequest.reasonCode"
			},
			{
				"id": "MedicationRequest.reasonCode.coding",
				"path": "MedicationRequest.reasonCode.coding",
				"min": 1,
				"max": "*"
			},
			{
				"id": "MedicationRequest.dispenseRequest",
				"path": "MedicationRequest.dispenseRequest",
				"label": "Mandatory dispense request",
				"short": "Mandatory dispense request",
				"min": 1,
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.substitution",
				"path": "MedicationRequest.substitution",
				"label": "Substitution",
				"short": "Substitution",
				"min": 1,
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.substitution.allowed[x]",
				"path": "MedicationRequest.substitution.allowed[x]",
				"label": "Substitution Allowed",
				"short": "Substitution Allowed",
				"definition": "Whether or not the pharmacy may dispense a substitutable generic product instead of a requested branded product",
				"type": [
					{
						"code": "boolean"
					}
				],
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.quantity",
				"path": "MedicationRequest.dispenseRequest.quantity",
				"label": "Prescribed Quantity",
				"short": "Prescribed Quantity",
				"min": 1,
				"max": "1",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.expectedSupplyDuration",
				"path": "MedicationRequest.dispenseRequest.expectedSupplyDuration",
				"label": "Days Supply",
				"short": "Days Supply",
				"min": 1,
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.performer",
				"path": "MedicationRequest.dispenseRequest.performer",
				"label": "Pharmacy",
				"short": "Pharmacy",
				"definition": "Proposed dispensing pharmacy.",
				"min": 1,
				"type": [
					{
						"code": "Reference",
						"targetProfile": [
							"http://hl7.org/fhir/us/carin/StructureDefinition/rtpbc-pharmacy"
						]
					}
				],
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.performer.reference",
				"path": "MedicationRequest.dispenseRequest.performer.reference",
				"label": "Pharmacy Reference",
				"short": "Pharmacy Reference",
				"min": 1,
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.expectedSupplyDuration.value",
				"path": "MedicationRequest.dispenseRequest.expectedSupplyDuration.value",
				"label": "Days Supply Value",
				"short": "Days Supply Value",
				"definition": "The number of days the requested dispense quantity is intended to last",
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.quantity.value",
				"path": "MedicationRequest.dispenseRequest.quantity.value",
				"label": "Quantity in Billing Units",
				"short": "Quantity in Billing Units",
				"definition": "The prescribed quantity in terms of the applicable billing unit of measure",
				"min": 1,
				"mustSupport": true
			},
			{
				"id": "MedicationRequest.dispenseRequest.quantity.unit",
				"path": "MedicationRequest.dispenseRequest.quantity.unit",
				"label": "Billing unit of measure",
				"short": "Billing unit of measure",
				"definition": "Pharmacy metric billing unit: GM (gram), ML (milliliter) or EA (each)",
				"min": 1,
				"mustSupport": true,
				"binding": {
					"strength": "required",
					"description": "Billing quantity unity of measure"
				}
			}
		]
	}
}